        </div>
        <div class="container">
            <div class="row center">
                <% @users.each do |user| %>
                    <div data-no-turbolink class="ekipa_display">  
                    
                        <%= image_tag(user.img.url(:thumb), :class => "img-responsive img-circle member_foto", :target => '_blank') %>
                        <h3><%=user.name%> <%=user.surname%></h3>
                        
                        <div class="inline">
                            <p><%= user.text %></p>
                            <h2>
                                <% if !user.email.blank? %>
                                    <%= mail_to(user.email) do %><span class="icon iconn-envelop"></span><% end %>
                                <% end %>
                                <% if !user.link.blank? %>
                                    <%= link_to(user.link, :target => '_blank') do %><span class="icon iconn-facebook2"></span><% end %>
                                <% end %>
                            </h2>
                        </div>
                    </div>
                <% end %>

            </div>
        </div>                    
